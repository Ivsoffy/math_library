#suite exp_tests
#test exp_test_1
  ck_assert_ldouble_eq_tol(s21_exp(1), expl(1), S21_EPS);

#test exp_test_2
  ck_assert_ldouble_eq_tol(s21_exp(-1), expl(-1), S21_EPS);

#test exp_test_3
  ck_assert_ldouble_eq(s21_exp(0), expl(0));

#test exp_test_4
  ck_assert_ldouble_eq(s21_exp(-0), expl(-0));

#test exp_test_5_inf
  ck_assert_ldouble_eq(s21_exp(999999999999), expl(999999999999));

#test exp_test_6
  ck_assert_ldouble_eq_tol(s21_exp(-999999999999), expl(-999999999999), S21_EPS);

#test exp_test_7_inf
  ck_assert_ldouble_eq(s21_exp(DBL_MAX), expl(DBL_MAX));

#test exp_test_8
  ck_assert_ldouble_eq_tol(s21_exp(-DBL_MAX), expl(-DBL_MAX), S21_EPS);

#test exp_test_9
  ck_assert_ldouble_eq_tol(s21_exp(1e-9), expl(1e-9), S21_EPSL);

#test exp_test_10
  ck_assert_ldouble_eq_tol(s21_exp(-1e-9), expl(-1e-9), S21_EPSL);

#test exp_test_11
  ck_assert_ldouble_eq_tol(s21_exp(DBL_MIN), expl(DBL_MIN), S21_EPS);

#test exp_test_12
  ck_assert_ldouble_eq_tol(s21_exp(DBL_MIN), expl(DBL_MIN), S21_EPS);

#test exp_test_13
  ck_assert_ldouble_eq_tol(s21_exp(-DBL_MIN), expl(-DBL_MIN), S21_EPS);

#test exp_test_14
  ck_assert_ldouble_eq(s21_exp(-S21_INF), expl(-S21_INF));

#test exp_test_15_inf
  ck_assert_ldouble_eq(s21_exp(S21_INF), expl(S21_INF));

#test exp_test_16_nan
  ck_assert_ldouble_nan(s21_exp(S21_NAN));
  ck_assert_ldouble_nan(expl(S21_NAN));
