#suite sin_tests
#test sin_test_1
  for (double i = -50.0; i < 50.1; i += 0.1) {
    ck_assert_ldouble_eq_tol(s21_sin(i), sinl(i), S21_EPS);
  }

#test sin_test_2
  ck_assert_ldouble_eq_tol(s21_sin(9999432424.124241), sinl(9999432424.124241), S21_EPS);

#test sin_test_3
  ck_assert_ldouble_eq_tol(s21_sin(-332432.4252), sinl(-332432.4252), S21_EPS);

#test sin_test_4
  ck_assert_ldouble_eq_tol(s21_sin(0), sinl(0), S21_EPS);

#test sin_test_5
  ck_assert_ldouble_eq_tol(s21_sin(1), sinl(1), S21_EPS);

#test sin_test_6
  ck_assert_ldouble_eq_tol(s21_sin(-1), sinl(-1), S21_EPS);

#test sin_test_7_nan
  ck_assert_ldouble_nan(s21_sin(S21_INF));
  ck_assert_ldouble_nan(sinl(S21_INF));

#test sin_test_8_nan
  ck_assert_ldouble_nan(s21_sin(-S21_INF));
  ck_assert_ldouble_nan(sinl(-S21_INF));

#test sin_test_9_nan
  ck_assert_ldouble_nan(s21_sin(S21_NAN));
  ck_assert_ldouble_nan(sinl(S21_NAN));

#test sin_test_10
  ck_assert_ldouble_eq(s21_sin(DBL_MIN), sinl(DBL_MIN));
