#suite ceil_tests
#test ceil_test_1
  ck_assert_ldouble_eq(s21_ceil(0), ceill(0));
  ck_assert_ldouble_eq(s21_ceil(-0), ceill(-0));
  ck_assert_ldouble_eq(s21_ceil(-2432.3242), ceill(-2432.3242));

#test ceil_test_2
  ck_assert_ldouble_eq(s21_ceil(10101010.10), ceill(10101010.10));
  ck_assert_ldouble_eq(s21_ceil(-99.999), ceill(-99.999));
  ck_assert_ldouble_eq(s21_ceil(999999.9789798), ceill(999999.9789798));
  ck_assert_ldouble_eq(s21_ceil(-56463.0), ceill(-56463.0));

#test ceil_test_3
  ck_assert_ldouble_eq(s21_ceil(00034554.00034554), ceill(00034554.00034554));
  ck_assert_ldouble_eq(s21_ceil(-0.00000001), ceill(-0.00000001));

#test ceil_test_4_nan
  ck_assert_ldouble_nan(s21_ceil(S21_NAN));
  ck_assert_ldouble_nan(ceill(S21_NAN));

#test ceil_test_5_inf
  ck_assert_ldouble_eq(s21_ceil(S21_INF), ceill(S21_INF));
  ck_assert_ldouble_eq(s21_ceil(-S21_INF), ceill(-S21_INF));

#test ceil_test_5
  ck_assert_ldouble_eq(s21_ceil(DBL_MIN), ceill(DBL_MIN));
  ck_assert_ldouble_eq(s21_ceil(-DBL_MIN), ceill(-DBL_MIN));
