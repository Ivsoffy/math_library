#suite atan_tests
#test atan_test_1
  ck_assert_ldouble_eq_tol(s21_atan(0.0), atanl(0.0), S21_EPS);

#test atan_test_2
  ck_assert_ldouble_eq_tol(s21_atan(-0.0), atanl(-0.0), S21_EPS);

#test atan_test_3
  ck_assert_ldouble_eq_tol(s21_atan(-1), atanl(-1), S21_EPS);

#test atan_test_4
  ck_assert_ldouble_eq_tol(s21_atan(1), atanl(1), S21_EPS);

#test atan_test_5
  ck_assert_ldouble_eq_tol(s21_atan(S21_INF), atanl(S21_INF), S21_EPS);

#test atan_test_6
  ck_assert_ldouble_eq_tol(s21_atan(-S21_INF), atanl(-S21_INF), S21_EPS);

#test atan_test_7
  ck_assert_ldouble_nan(s21_atan(S21_NAN));
  ck_assert_ldouble_nan(atanl(S21_NAN));

#test atan_test_8
  ck_assert_ldouble_eq_tol(s21_atan(DBL_MAX), atanl(DBL_MAX), S21_EPS);

#test atan_test_9
  ck_assert_ldouble_eq_tol(s21_atan(DBL_MIN), atanl(DBL_MIN), S21_EPS);

#test atan_test_10
  ck_assert_ldouble_eq_tol(s21_atan(42352), atanl(42352), S21_EPS);

#test atan_test_11
  ck_assert_ldouble_eq_tol(s21_atan(-35363), atanl(-35363), S21_EPS);

#test atan_test_12
  ck_assert_ldouble_eq_tol(s21_atan(0.666666), atanl(0.666666), S21_EPS);

#test atan_test_13
  ck_assert_ldouble_eq_tol(s21_atan(-0.333333), atanl(-0.333333), S21_EPS);

#test atan_test_14
  ck_assert_ldouble_eq_tol(s21_atan(57434.574554256), atanl(57434.574554256), S21_EPS);

#test atan_test_15
  ck_assert_ldouble_eq_tol(s21_atan(-4.574256), atanl(-4.574256), S21_EPS);
