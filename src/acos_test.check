#suite acos_tests
#test acos_test_1
  ck_assert_ldouble_eq_tol(s21_acos(0.0), acosl(0.0), S21_EPS);

#test acos_test_2
  ck_assert_ldouble_eq_tol(s21_acos(-0.0), acosl(-0.0), S21_EPS);

#test acos_test_3
  ck_assert_ldouble_eq_tol(s21_acos(-1), acosl(-1), S21_EPS);

#test acos_test_4
  ck_assert_ldouble_eq_tol(s21_acos(1), acosl(1), S21_EPS);

#test acos_test_5
  ck_assert_ldouble_nan(s21_acos(S21_INF));
  ck_assert_ldouble_nan(acosl(S21_INF));

#test acos_test_6
  ck_assert_ldouble_nan(s21_acos(-S21_INF));
  ck_assert_ldouble_nan(acosl(-S21_INF));

#test acos_test_7
  ck_assert_ldouble_nan(s21_acos(S21_NAN));
  ck_assert_ldouble_nan(acosl(S21_NAN));

#test acos_test_8
  ck_assert_ldouble_nan(s21_acos(DBL_MAX));
  ck_assert_ldouble_nan(acosl(DBL_MAX));

#test acos_test_9
  ck_assert_ldouble_eq_tol(s21_acos(DBL_MIN), acos(DBL_MIN), S21_EPS);

#test acos_test_10
  ck_assert_ldouble_nan(s21_acos(42352));
  ck_assert_ldouble_nan(acosl(42352));

#test acos_test_11
  ck_assert_ldouble_nan(s21_acos(-35363));
  ck_assert_ldouble_nan(acosl(-35363));

#test acos_test_12
  ck_assert_ldouble_eq_tol(s21_acos(0.666666), acosl(0.666666), S21_EPS);

#test acos_test_13
  ck_assert_ldouble_eq_tol(s21_acos(-0.333333), acosl(-0.333333), S21_EPS);

#test acos_test_14
  ck_assert_ldouble_eq_tol(s21_acos(0.999999), acosl(0.999999), S21_EPS);

#test acos_test_15
  ck_assert_ldouble_eq_tol(s21_acos(-0.888), acosl(-0.888), S21_EPS);
