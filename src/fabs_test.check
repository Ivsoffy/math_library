#suite fabs_tests
#test fabs_test_1
  ck_assert_ldouble_eq(s21_fabs(-0.4634985729), fabsl(-0.4634985729));
  ck_assert_ldouble_eq(s21_fabs(1000.10000), fabsl(1000.1000));
  ck_assert_ldouble_eq(s21_fabs(12345.12345), fabsl(12345.12345));
  ck_assert_ldouble_eq(s21_fabs(-12345.12345), fabsl(-12345.12345));

#test fabs_test_2
  ck_assert_ldouble_eq(s21_fabs(DBL_MIN), fabsl(DBL_MIN));
  ck_assert_ldouble_eq(s21_fabs(-DBL_MIN), fabsl(-DBL_MIN));
  ck_assert_ldouble_eq(s21_fabs(DBL_MAX), fabsl(DBL_MAX));
  ck_assert_ldouble_eq(s21_fabs(-DBL_MAX), fabsl(-DBL_MAX));

#test fabs_test_4
  ck_assert_ldouble_nan(s21_fabs(S21_NAN));
  ck_assert_ldouble_nan(fabsl(S21_NAN));

#test fabs_test_5
  ck_assert_ldouble_eq(s21_fabs(S21_INF), fabsl(S21_INF)); 
  ck_assert_ldouble_eq(s21_fabs(-S21_INF), fabsl(-S21_INF));

#test fabs_test_6
  ck_assert_ldouble_eq(s21_fabs(0.0), fabsl(0.0)); 
  ck_assert_ldouble_eq(s21_fabs(-0.0), fabsl(-0.0));
